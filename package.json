{
  "name": "@fahmi-armand/packagetest",
  "version": "0.0.1",
  "description": "Testing publish package for npm via github",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prettier": "prettier src --check",
    "prettier-fix": "prettier src --write",
    "lint": "./node_modules/.bin/eslint src --ext .js,.ts,.tsx",
    "lint-fix": "./node_modules/.bin/eslint --fix src --ext .js,.ts,.tsx",
    "generate-docs": "npx typedoc --entryPointStrategy expand ./src",
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fahmi-armand/packagetest.git"
  },
  "keywords": [
    "typescript"
  ],
  "author": "Fahmi",
  "license": "MIT",
  "devDependencies": {
    "@types/xmlrpc": "^1.3.10",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "dotenv": "^16.4.5"
  }
}
